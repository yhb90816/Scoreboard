package chapter1;

public class Hello {
	public static void main(String[]args)
	{
		int option=1;
		switch(option) {
	    case 1: System.out.println("可记 1");
	    case 2: System.out.println("可记 2");
	    case 3: System.out.println("可记 3"); break;
	    default: System.out.println("秦寸 绝澜");
	}

	}

}
